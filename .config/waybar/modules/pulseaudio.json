{
    "pulseaudio": {
		"scroll-step": 1,
		"format": "<span color='#b45bcf'>{icon}</span> {volume}%",
		"format-muted": "<span color='#ea51b2'></span>",
		"format-icons": {
			"headphone": "",
			"hands-free": "󱡏",
			"headset": "󰋏",
			"phone": "",
			"portable": "",
			"car": "",
			"default": [
				"",
				"",
				""
			]
		},
		"on-click": "pavucontrol",
		"on-scroll-up": "pamixer -ui 1",
		"on-scroll-down": "pamixer -ud 1"
	},
	"pulseaudio#source": {
		"format": "{format_source}",
		"format-source": "<span color='#b45bcf'></span> {volume}%",
		"format-source-muted": "<span color='#ea51b2'></span>",
		"on-click": "pamixer --default-source -t",
		"on-scroll-up": "pamixer --default-source -ui 1",
		"on-scroll-down": "pamixer --default-source -ud 1"
	}
}
